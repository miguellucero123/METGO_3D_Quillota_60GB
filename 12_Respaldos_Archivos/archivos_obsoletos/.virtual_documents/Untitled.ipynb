def interpretar_r2_datos_reales(r2_score):
    """
    Interpreta el R¬≤ espec√≠ficamente para datos meteorol√≥gicos reales
    """
    
    if r2_score >= 0.9:
        return "ü•á Excelente - Modelo muy preciso con datos reales"
    elif r2_score >= 0.8:
        return "ü•à Muy bueno - Precisi√≥n alta con datos reales" 
    elif r2_score >= 0.7:
        return "ü•â Bueno - Precisi√≥n aceptable con datos reales"
    elif r2_score >= 0.5:
        return "‚ö†Ô∏è Regular - Necesita mejoras con datos reales"
    elif r2_score >= 0.0:
        return "üî¥ Bajo - Revisar caracter√≠sticas con datos reales"
    else:
        return "‚ùå Muy bajo - Modelo peor que media con datos reales"

def comparar_datos_reales_vs_sinteticos():
    """
    Compara resultados entre datos reales y sint√©ticos
    """
    
    print("üîÑ COMPARACI√ìN: DATOS REALES vs SINT√âTICOS")
    print("-"*50)
    
    try:
        datos_reales = globals().get('datos_meteorologicos')
        datos_sinteticos = globals().get('datos_meteorologicos_backup')
        
        if datos_reales is not None and datos_sinteticos is not None:
            
            # Comparar estad√≠sticas b√°sicas
            comparaciones = []
            
            for col in ['temperatura_max', 'temperatura_min', 'precipitacion', 'humedad_relativa']:
                if col in datos_reales.columns and col in datos_sinteticos.columns:
                    real_mean = datos_reales[col].mean()
                    sint_mean = datos_sinteticos[col].mean()
                    diferencia = abs(real_mean - sint_mean)
                    
                    comparaciones.append({
                        'variable': col,
                        'real': real_mean,
                        'sintetico': sint_mean, 
                        'diferencia': diferencia
                    })
            
            print("üìä Estad√≠sticas comparativas:")
            for comp in comparaciones:
                print(f"   {comp['variable']}:")
                print(f"      Real: {comp['real']:.1f}")
                print(f"      Sint√©tico: {comp['sintetico']:.1f}")
                print(f"      Diferencia: {comp['diferencia']:.1f}")
            
            # Evaluar realismo de datos sint√©ticos
            diferencias_promedio = np.mean([c['diferencia'] for c in comparaciones])
            
            if diferencias_promedio <= 2:
                print("‚úÖ Datos sint√©ticos muy similares a los reales")
            elif diferencias_promedio <= 5:
                print("üü° Datos sint√©ticos moderadamente similares a los reales")
            else:
                print("üî¥ Datos sint√©ticos difieren significativamente de los reales")
        
        else:
            print("‚ö†Ô∏è No se pueden comparar datos (uno o ambos conjuntos faltantes)")
            
    except Exception as e:
        print(f"‚ùå Error en comparaci√≥n: {e}")

