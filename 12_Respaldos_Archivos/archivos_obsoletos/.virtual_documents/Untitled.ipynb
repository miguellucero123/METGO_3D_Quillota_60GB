def importar_datos_openmeteo(latitud=-32.8833, longitud=-71.25, dias_historicos=365, incluir_pronostico=True):
    """
    Importa datos meteorol√≥gicos reales desde OpenMeteo API
    
    Par√°metros:
    -----------
    latitud : float
        Latitud de Quillota (-32.8833)
    longitud : float  
        Longitud de Quillota (-71.25)
    dias_historicos : int
        N√∫mero de d√≠as hist√≥ricos a obtener
    incluir_pronostico : bool
        Si incluir pron√≥stico de 7 d√≠as
    
    Retorna:
    --------
    pandas.DataFrame
        DataFrame con datos meteorol√≥gicos reales
    """
    
    print("üåê CONECTANDO CON OPENMETEO API")
    print(f"üìç Ubicaci√≥n: Quillota ({latitud}, {longitud})")
    print("="*60)
    
    try:
        import requests
        from datetime import datetime, timedelta
        
        # Fechas para datos hist√≥ricos
        fecha_fin = datetime.now().date()
        fecha_inicio = fecha_fin - timedelta(days=dias_historicos)
        
        print(f"üìÖ Per√≠odo hist√≥rico: {fecha_inicio} a {fecha_fin}")
        
        # URL base de OpenMeteo
        url_base = "https://api.open-meteo.com/v1/forecast"
        
        # Par√°metros para la API
        parametros = {
            'latitude': latitud,
            'longitude': longitud,
            'start_date': fecha_inicio.strftime('%Y-%m-%d'),
            'end_date': fecha_fin.strftime('%Y-%m-%d'),
            'daily': [
                'temperature_2m_max',
                'temperature_2m_min', 
                'temperature_2m_mean',
                'relative_humidity_2m',
                'precipitation_sum',
                'windspeed_10m_max',
                'pressure_msl_mean',
                'sunshine_duration',
                'et0_fao_evapotranspiration'
            ],
            'timezone': 'America/Santiago'
        }
        
        print("üîÑ Realizando solicitud a OpenMeteo...")
        print(f"   Variables solicitadas: {len(parametros['daily'])}")
        
        # Realizar solicitud HTTP
        response = requests.get(url_base, params=parametros, timeout=30)
        
        if response.status_code == 200:
            datos_json = response.json()
            print("‚úÖ Datos recibidos exitosamente")
            
            # Procesar respuesta JSON
            datos_diarios = datos_json.get('daily', {})
            fechas = datos_diarios.get('time', [])
            
            if not fechas:
                print("‚ùå No se recibieron fechas en la respuesta")
                return None
            
            print(f"üìä Procesando {len(fechas)} d√≠as de datos...")
            
            # Crear DataFrame con datos reales
            datos_procesados = []
            
            for i, fecha_str in enumerate(fechas):
                try:
                    fecha = pd.to_datetime(fecha_str)
                    
                    registro = {
                        'fecha': fecha,
                        'temperatura_max': datos_diarios.get('temperature_2m_max', [None] * len(fechas))[i],
                        'temperatura_min': datos_diarios.get('temperature_2m_min', [None] * len(fechas))[i],
                        'temperatura_promedio': datos_diarios.get('temperature_2m_mean', [None] * len(fechas))[i],
                        'humedad_relativa': datos_diarios.get('relative_humidity_2m', [None] * len(fechas))[i],
                        'precipitacion': datos_diarios.get('precipitation_sum', [None] * len(fechas))[i],
                        'velocidad_viento': datos_diarios.get('windspeed_10m_max', [None] * len(fechas))[i],
                        'presion_atmosferica': datos_diarios.get('pressure_msl_mean', [None] * len(fechas))[i],
                        'horas_sol': datos_diarios.get('sunshine_duration', [None] * len(fechas))[i],
                        'evapotranspiracion': datos_diarios.get('et0_fao_evapotranspiration', [None] * len(fechas))[i]
                    }
                    
                    # Validar que al menos temperatura est√© disponible
                    if registro['temperatura_max'] is not None or registro['temperatura_min'] is not None:
                        datos_procesados.append(registro)
                
                except Exception as e:
                    print(f"‚ö†Ô∏è Error procesando fecha {fecha_str}: {e}")
                    continue
            
            if not datos_procesados:
                print("‚ùå No se pudieron procesar los datos recibidos")
                return None
            
            # Crear DataFrame
            df_openmeteo = pd.DataFrame(datos_procesados)
            
            # Estad√≠sticas de datos recibidos
            print(f"‚úÖ DATOS OPENMETEO PROCESADOS:")
            print(f"   üìä Total registros: {len(df_openmeteo)}")
            print(f"   üìÖ Rango: {df_openmeteo['fecha'].min().strftime('%Y-%m-%d')} a {df_openmeteo['fecha'].max().strftime('%Y-%m-%d')}")
            
            # Mostrar estad√≠sticas por variable
            for col in ['temperatura_max', 'temperatura_min', 'humedad_relativa', 'precipitacion']:
                if col in df_openmeteo.columns:
                    valores_validos = df_openmeteo[col].dropna()
                    if len(valores_validos) > 0:
                        print(f"   üîπ {col}: {len(valores_validos)} valores v√°lidos (min: {valores_validos.min():.1f}, max: {valores_validos.max():.1f})")
                    else:
                        print(f"   ‚ö†Ô∏è {col}: Sin datos v√°lidos")
            
            # Obtener pron√≥stico si se solicita
            if incluir_pronostico:
                print("\nüîÆ Obteniendo pron√≥stico de 7 d√≠as...")
                pronostico = obtener_pronostico_openmeteo(latitud, longitud, dias=7)
                
                if pronostico is not None and len(pronostico) > 0:
                    # Combinar datos hist√≥ricos con pron√≥stico
                    df_combinado = pd.concat([df_openmeteo, pronostico], ignore_index=True)
                    df_combinado = df_combinado.drop_duplicates(subset=['fecha'], keep='first')
                    df_combinado = df_combinado.sort_values('fecha').reset_index(drop=True)
                    
                    print(f"   ‚úÖ Pron√≥stico agregado: {len(pronostico)} d√≠as")
                    print(f"   üìä Total con pron√≥stico: {len(df_combinado)} registros")
                    
                    return df_combinado
            
            return df_openmeteo
            
        else:
            print(f"‚ùå Error en solicitud HTTP: {response.status_code}")
            print(f"   Respuesta: {response.text[:200]}...")
            return None
            
    except requests.exceptions.RequestException as e:
        print(f"‚ùå Error de conexi√≥n: {e}")
        print("üí° Verificar conexi√≥n a internet")
        return None
        
    except Exception as e:
        print(f"‚ùå Error inesperado: {e}")
        import traceback
        traceback.print_exc()
        return None

def obtener_pronostico_openmeteo(latitud=-32.8833, longitud=-71.25, dias=7):
    """
    Obtiene pron√≥stico meteorol√≥gico desde OpenMeteo
    
    Par√°metros:
    -----------
    latitud : float
        Latitud de la ubicaci√≥n
    longitud : float
        Longitud de la ubicaci√≥n  
    dias : int
        N√∫mero de d√≠as de pron√≥stico
    
    Retorna:
    --------
    pandas.DataFrame
        DataFrame con pron√≥stico meteorol√≥gico
    """
    
    try:
        import requests
        from datetime import datetime, timedelta
        
        # Fechas para pron√≥stico (desde hoy hacia adelante)
        fecha_inicio = datetime.now().date()
        fecha_fin = fecha_inicio + timedelta(days=dias)
        
        print(f"   üìÖ Pron√≥stico: {fecha_inicio} a {fecha_fin}")
        
        # URL para pron√≥stico
        url_forecast = "https://api.open-meteo.com/v1/forecast"
        
        parametros_forecast = {
            'latitude': latitud,
            'longitude': longitud,
            'daily': [
                'temperature_2m_max',
                'temperature_2m_min',
                'temperature_2m_mean', 
                'relative_humidity_2m',
                'precipitation_sum',
                'windspeed_10m_max',
                'pressure_msl_mean'
            ],
            'timezone': 'America/Santiago',
            'forecast_days': dias
        }
        
        response = requests.get(url_forecast, params=parametros_forecast, timeout=15)
        
        if response.status_code == 200:
            datos_json = response.json()
            datos_diarios = datos_json.get('daily', {})
            fechas = datos_diarios.get('time', [])
            
            pronostico_procesado = []
            
            for i, fecha_str in enumerate(fechas):
                try:
                    fecha = pd.to_datetime(fecha_str)
                    
                    registro = {
                        'fecha': fecha,
                        'temperatura_max': datos_diarios.get('temperature_2m_max', [None] * len(fechas))[i],
                        'temperatura_min': datos_diarios.get('temperature_2m_min', [None] * len(fechas))[i],
                        'temperatura_promedio': datos_diarios.get('temperature_2m_mean', [None] * len(fechas))[i],
                        'humedad_relativa': datos_diarios.get('relative_humidity_2m', [None] * len(fechas))[i],
                        'precipitacion': datos_diarios.get('precipitation_sum', [None] * len(fechas))[i],
                        'velocidad_viento': datos_diarios.get('windspeed_10m_max', [None] * len(fechas))[i],
                        'presion_atmosferica': datos_diarios.get('pressure_msl_mean', [None] * len(fechas))[i],
                        'es_pronostico': True  # Marcar como datos de pron√≥stico
                    }
                    
                    pronostico_procesado.append(registro)
                    
                except Exception as e:
                    print(f"   ‚ö†Ô∏è Error procesando pron√≥stico para {fecha_str}: {e}")
                    continue
            
            if pronostico_procesado:
                return pd.DataFrame(pronostico_procesado)
            else:
                return None
                
        else:
            print(f"   ‚ùå Error obteniendo pron√≥stico: {response.status_code}")
            return None
            
    except Exception as e:
        print(f"   ‚ùå Error en pron√≥stico: {e}")
        return None

def cargar_datos_con_openmeteo(usar_cache=True, forzar_actualizacion=False):
    """
    Funci√≥n principal para cargar datos combinando OpenMeteo con cache local
    
    Par√°metros:
    -----------
    usar_cache : bool
        Si usar datos en cache cuando est√©n disponibles
    forzar_actualizacion : bool
        Si forzar actualizaci√≥n desde OpenMeteo
    
    Retorna:
    --------
    pandas.DataFrame
        DataFrame con datos meteorol√≥gicos actualizados
    """
    
    print("üìä CARGANDO DATOS METEOROL√ìGICOS PARA QUILLOTA")
    print("="*60)
    
    datos_finales = None
    fuente_datos = "desconocida"
    
    # Intentar cargar desde cache primero
    if usar_cache and not forzar_actualizacion:
        print("üíæ Verificando cache local...")
        
        # Buscar archivos de cache existentes
        import os
        import glob
        
        archivos_cache = glob.glob("datos_*_quillota_*.csv")
        archivos_cache.extend(glob.glob("datos_*_quillota_*.xlsx"))
        
        if archivos_cache:
            # Usar el archivo m√°s reciente
            archivo_mas_reciente = max(archivos_cache, key=os.path.getctime)
            print(f"   ‚úÖ Cache encontrado: {archivo_mas_reciente}")
            
            try:
                if archivo_mas_reciente.endswith('.csv'):
                    datos_cache = pd.read_csv(archivo_mas_reciente, parse_dates=['fecha'])
                else:
                    datos_cache = pd.read_excel(archivo_mas_reciente, parse_dates=['fecha'])
                
                # Verificar edad del cache (m√°ximo 1 d√≠a)
                fecha_archivo = datetime.fromtimestamp(os.path.getctime(archivo_mas_reciente))
                edad_cache = (datetime.now() - fecha_archivo).total_seconds() / 3600  # horas
                
                if edad_cache < 24:  # Cache v√°lido por 24 horas
                    print(f"   ‚úÖ Cache v√°lido (edad: {edad_cache:.1f} horas)")
                    datos_finales = datos_cache
                    fuente_datos = "cache_local"
                else:
                    print(f"   ‚ö†Ô∏è Cache obsoleto (edad: {edad_cache:.1f} horas)")
            
            except Exception as e:
                print(f"   ‚ùå Error leyendo cache: {e}")
    
    # Si no hay cache v√°lido, obtener desde OpenMeteo
    if datos_finales is None or forzar_actualizacion:
        print("üåê Obteniendo datos desde OpenMeteo...")
        
        # Configuraci√≥n para Quillota
        QUILLOTA_LAT = -32.8833
        QUILLOTA_LON = -71.25
        
        datos_openmeteo = importar_datos_openmeteo(
            latitud=QUILLOTA_LAT,
            longitud=QUILLOTA_LON,
            dias_historicos=365,
            incluir_pronostico=True
        )
        
        if datos_openmeteo is not None:
            datos_finales = datos_openmeteo
            fuente_datos = "openmeteo_api"
            
            # Guardar en cache para uso futuro
            print("üíæ Guardando en cache...")
            try:
                timestamp = datetime.now().strftime("%Y%m%d_%H%M%S")
                archivo_cache = f"datos_openmeteo_quillota_{timestamp}.csv"
                datos_openmeteo.to_csv(archivo_cache, index=False)
                print(f"   ‚úÖ Cache guardado: {archivo_cache}")
            except Exception as e:
                print(f"   ‚ö†Ô∏è Error guardando cache: {e}")
        
        else:
            print("‚ùå No se pudieron obtener datos desde OpenMeteo")
    
    # Si todo falla, usar datos sint√©ticos como respaldo
    if datos_finales is None:
        print("üîÑ Generando datos sint√©ticos como respaldo...")
        
        try:
            # Usar la funci√≥n existente de datos sint√©ticos
            if 'generar_datos_meteorologicos' in globals():
                datos_finales = generar_datos_meteorologicos(dias=365, incluir_extremos=True)
                fuente_datos = "sintetico_respaldo"
            else:
                # Crear datos b√°sicos si la funci√≥n no existe
                fechas = pd.date_range(start='2024-01-01', periods=365, freq='D')
                np.random.seed(42)
                
                datos_finales = pd.DataFrame({
                    'fecha': fechas,
                    'temperatura_max': np.random.normal(20, 8, 365),
                    'temperatura_min': np.random.normal(10, 5, 365),
                    'humedad_relativa': np.random.uniform(30, 95, 365),
                    'precipitacion': np.random.exponential(1, 365),
                    'velocidad_viento': np.random.gamma(2, 3, 365),
                    'presion_atmosferica': np.random.normal(1013, 15, 365)
                })
                fuente_datos = "sintetico_basico"
            
            print("   ‚úÖ Datos sint√©ticos generados como respaldo")
            
        except Exception as e:
            print(f"   ‚ùå Error generando datos sint√©ticos: {e}")
            return None
    
    # Procesar y validar datos finales
    if datos_finales is not None:
        print(f"\n‚úÖ DATOS CARGADOS EXITOSAMENTE")
        print(f"üìç Fuente: {fuente_datos}")
        print(f"üìä Registros: {len(datos_finales)}")
        
        if 'fecha' in datos_finales.columns:
            print(f"üìÖ Rango: {datos_finales['fecha'].min().strftime('%Y-%m-%d')} a {datos_finales['fecha'].max().strftime('%Y-%m-%d')}")
        
        # Estad√≠sticas r√°pidas
        columnas_numericas = datos_finales.select_dtypes(include=[np.number]).columns
        print(f"üî¢ Variables num√©ricas: {len(columnas_numericas)}")
        
        # Verificar completitud
        completitud = (datos_finales.isnull().sum() / len(datos_finales) * 100).round(1)
        columnas_completas = (completitud == 0).sum()
        print(f"‚úÖ Columnas completas: {columnas_completas}/{len(datos_finales.columns)}")
        
        # Marcar fuente en los datos
        datos_finales['fuente_datos'] = fuente_datos
        
        # Actualizar variable global
        globals()['datos_meteorologicos'] = datos_finales
        
        print("üåæ Datos meteorol√≥gicos listos para agricultura de precisi√≥n")
        
        return datos_finales
    
    else:
        print("‚ùå No se pudieron cargar datos desde ninguna fuente")
        return None

