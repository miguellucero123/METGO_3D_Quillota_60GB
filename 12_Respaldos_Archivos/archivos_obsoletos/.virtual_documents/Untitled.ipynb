def configurar_actualizacion_automatica(intervalo_horas=24):
    """
    Configura actualizaci√≥n autom√°tica de datos desde OpenMeteo
    
    Par√°metros:
    -----------
    intervalo_horas : int
        Intervalo en horas para actualizar datos
    """
    
    print(f"‚è∞ CONFIGURANDO ACTUALIZACI√ìN AUTOM√ÅTICA")
    print(f"üîÑ Intervalo: cada {intervalo_horas} horas")
    print("="*50)
    
    try:
        # Guardar configuraci√≥n en variable global
        config_auto = {
            'habilitado': True,
            'intervalo_horas': intervalo_horas,
            'ultima_actualizacion': pd.Timestamp.now(),
            'proxima_actualizacion': pd.Timestamp.now() + pd.Timedelta(hours=intervalo_horas),
            'actualizaciones_exitosas': 0,
            'actualizaciones_fallidas': 0
        }
        
        globals()['CONFIG_ACTUALIZACION_AUTO'] = config_auto
        
        print("‚úÖ Configuraci√≥n de actualizaci√≥n autom√°tica guardada")
        print(f"üìÖ Pr√≥xima actualizaci√≥n: {config_auto['proxima_actualizacion'].strftime('%Y-%m-%d %H:%M:%S')}")
        
        # Crear funci√≥n de verificaci√≥n
        def verificar_actualizacion_necesaria():
            """Verifica si es necesario actualizar datos"""
            
            config = globals().get('CONFIG_ACTUALIZACION_AUTO')
            if not config or not config.get('habilitado', False):
                return False
            
            ahora = pd.Timestamp.now()
            return ahora >= config.get('proxima_actualizacion', ahora)
        
        globals()['verificar_actualizacion_necesaria'] = verificar_actualizacion_necesaria
        
        print("üîß Funci√≥n de verificaci√≥n creada")
        print("üí° Use verificar_actualizacion_necesaria() para comprobar si necesita actualizaci√≥n")
        
        return True
        
    except Exception as e:
        print(f"‚ùå Error configurando actualizaci√≥n autom√°tica: {e}")
        return False

def ejecutar_actualizacion_si_necesaria():
    """
    Ejecuta actualizaci√≥n de datos si es necesario seg√∫n configuraci√≥n
    """
    
    try:
        # Verificar si hay configuraci√≥n de actualizaci√≥n
        if 'CONFIG_ACTUALIZACION_AUTO' not in globals():
            return False
        
        config = globals()['CONFIG_ACTUALIZACION_AUTO']
        
        if not config.get('habilitado', False):
            return False
        
        # Verificar si es necesario actualizar
        if 'verificar_actualizacion_necesaria' in globals():
            necesita_actualizacion = globals()['verificar_actualizacion_necesaria']()
            
            if necesita_actualizacion:
                print("üîÑ Actualizaci√≥n autom√°tica necesaria - ejecutando...")
                
                # Intentar actualizaci√≥n
                resultado = cargar_datos_con_openmeteo(usar_cache=False, forzar_actualizacion=True)
                
                if resultado is not None:
                    print("‚úÖ Actualizaci√≥n autom√°tica exitosa")
                    
                    # Actualizar configuraci√≥n
                    config['ultima_actualizacion'] = pd.Timestamp.now()
                    config['proxima_actualizacion'] = pd.Timestamp.now() + pd.Timedelta(hours=config['intervalo_horas'])
                    config['actualizaciones_exitosas'] += 1
                    
                    print(f"üìÖ Pr√≥xima actualizaci√≥n: {config['proxima_actualizacion'].strftime('%Y-%m-%d %H:%M:%S')}")
                    
                    return True
                
                else:
                    print("‚ùå Actualizaci√≥n autom√°tica fall√≥")
                    config['actualizaciones_fallidas'] += 1
                    
                    return False
            else:
                return False  # No necesitaba actualizaci√≥n
        
        return False
        
    except Exception as e:
        print(f"‚ùå Error en actualizaci√≥n autom√°tica: {e}")
        return False

