        # Analizar l√≠nea problem√°tica y contexto
        problem_line_idx = 476  # l√≠nea 477 (√≠ndice 476)
        problem_line = lines[problem_line_idx]
        
        # Contexto alrededor del error (5 l√≠neas antes y despu√©s)
        context_start = max(0, problem_line_idx - 5)
        context_end = min(len(lines), problem_line_idx + 6)
        context_lines = lines[context_start:context_end]
        
        print(f"üìç L√çNEA PROBLEM√ÅTICA 477:")
        print(f"   ‚îÇ {problem_line}")
        print(f"   ‚îî‚îÄ Longitud: {len(problem_line)} caracteres")
        
        print(f"\nüìã CONTEXTO (l√≠neas {context_start + 1}-{context_end}):")
        for i, line in enumerate(context_lines, context_start + 1):
            marker = "üëâ" if i == 477 else "  "
            print(f"   {marker} {i:3d}: {line[:80]}{'...' if len(line) > 80 else ''}")
        
        # An√°lisis espec√≠fico de la l√≠nea problem√°tica
        analysis = {
            'line_content': problem_line,
            'line_length': len(problem_line),
            'parentheses_balance': problem_line.count('(') - problem_line.count(')'),
            'brackets_balance': problem_line.count('[') - problem_line.count(']'),
            'braces_balance': problem_line.count('{') - problem_line.count('}'),
            'single_quotes': problem_line.count("'"),
            'double_quotes': problem_line.count('"'),
            'has_colon': ':' in problem_line,
            'ends_with_comma': problem_line.rstrip().endswith(','),
            'potential_keywords': []
        }
        
        # Buscar keywords que podr√≠an necesitar ':'
        keywords_needing_colon = ['if', 'elif', 'else', 'for', 'while', 'def', 'class', 'try', 'except', 'finally', 'with']
        for keyword in keywords_needing_colon:
            if f' {keyword} ' in f' {problem_line} ' or problem_line.strip().startswith(f'{keyword} '):
                analysis['potential_keywords'].append(keyword)
        
        print(f"\nüîç AN√ÅLISIS DE LA L√çNEA 477:")
        print(f"   ‚Ä¢ Balance par√©ntesis: {analysis['parentheses_balance']} {'‚ö†Ô∏è' if analysis['parentheses_balance'] != 0 else '‚úÖ'}")
        print(f"   ‚Ä¢ Balance corchetes: {analysis['brackets_balance']} {'‚ö†Ô∏è' if analysis['brackets_balance'] != 0 else '‚úÖ'}")
        print(f"   ‚Ä¢ Balance llaves: {analysis['braces_balance']} {'‚ö†Ô∏è' if analysis['braces_balance'] != 0 else '‚úÖ'}")
        print(f"   ‚Ä¢ Comillas simples: {analysis['single_quotes']} {'‚ö†Ô∏è' if analysis['single_quotes'] % 2 != 0 else '‚úÖ'}")
        print(f"   ‚Ä¢ Comillas dobles: {analysis['double_quotes']} {'‚ö†Ô∏è' if analysis['double_quotes'] % 2 != 0 else '‚úÖ'}")
