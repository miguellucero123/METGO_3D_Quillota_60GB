        def subheader(self, text): print(f"\n{'-'*30}\nüìå {text}\n{'-'*30}")
        def metric(self, label, value, delta=None):
            delta_text = f" ({delta})" if delta else ""
            print(f"üìä {label}: {value}{delta_text}")
        def columns(self, n): return [self] * n
        def markdown(self, text): print(f"üìÑ {text}")
        def success(self, text): print(f"‚úÖ {text}")
        def warning(self, text): print(f"‚ö†Ô∏è {text}")
        def error(self, text): print(f"‚ùå {text}")
        def plotly_chart(self, fig, **kwargs): 
            try:
                fig.show()
            except:
                print("üìà Gr√°fico no disponible")
        def __getattr__(self, name): return lambda *args, **kwargs: self
    
