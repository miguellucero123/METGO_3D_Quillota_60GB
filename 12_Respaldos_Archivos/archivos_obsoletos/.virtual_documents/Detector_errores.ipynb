        'agricultura': {'cultivos_principales': ['paltas', 'citricos', 'uvas']}
    }
    
    UMBRALES_CRITICOS = {
        'temperatura': {'helada_severa': -2, 'helada_moderada': 0, 'calor_extremo': 35},
