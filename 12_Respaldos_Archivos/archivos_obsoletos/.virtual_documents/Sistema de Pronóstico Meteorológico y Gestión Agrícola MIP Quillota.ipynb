                    yaxis2=dict(title="Humedad (%)", side="right", overlaying="y"),
                    hovermode='x unified',
                    plot_bgcolor='rgba(0,0,0,0)',
                    paper_bgcolor='rgba(0,0,0,0)',
                    height=400
                )
                
                return fig
        
        # Si no hay datos reales, mostrar gráfico de estado
